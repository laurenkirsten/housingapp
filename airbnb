<script type="text/javascript">
$(document).ready(function(){
preloader();
$("#guests").change(function(){
	var guest=$("#guests").val();
	var temp_guest="";
	if(guest=="1")
	{
		temp_guest=guest+" "+"Guest";
	}
	else if(guest=="16")
	{
		temp_guest=guest+"+ "+"Guests";
	}
	else
	{
		temp_guest=guest+" "+"Guests";
	}
	$("#guests_caption").html(temp_guest);
});
})
