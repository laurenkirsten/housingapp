<script language="JavaScript">
function preloader() 
{
     // counter
     var i = 0;
     // create object
     imageObj = new Image();
     // set image list
     images = new Array();
	      images[0]="http://demo2server.in/sites/ci/airbnbclone/images/1/ef3e4692d7704ec4a205976290f7ceaf"
	      images[1]="http://demo2server.in/sites/ci/airbnbclone/images/8/23.jpg"
	      images[2]="http://demo2server.in/sites/ci/airbnbclone/images/2/14.jpg"
	      images[3]="http://demo2server.in/sites/ci/airbnbclone/images/3/17.jpg"
	      images[4]="http://demo2server.in/sites/ci/airbnbclone/images/64/php_love.jpg"
	      // start preloading
     for(i=0; i<=4; i++) 
     {
          imageObj.src=images[i];
     }
} 

